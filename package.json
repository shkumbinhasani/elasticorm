{
  "name": "@shkumbinhsn/elasticorm",
  "version": "0.2.1",
  "description": "A type-safe Elasticsearch ORM for TypeScript, inspired by Drizzle ORM",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun run clean && bun run build:tsc",
    "build:tsc": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:unit": "bun test tests/schema.test.ts tests/query.test.ts tests/aggregations.test.ts tests/builder.test.ts",
    "test:integration": "bun test tests/integration.test.ts",
    "typecheck": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "elasticsearch",
    "orm",
    "typescript",
    "type-safe",
    "query-builder",
    "drizzle",
    "elastic",
    "search",
    "database"
  ],
  "author": "Shkumbin Hasani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shkumbinhasani/elasticorm.git"
  },
  "bugs": {
    "url": "https://github.com/shkumbinhasani/elasticorm/issues"
  },
  "homepage": "https://github.com/shkumbinhasani/elasticorm#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18"
  }
}
